supDomain: Calibration
	domain: Mixer
		conf: Default
			component: /Audio/smartx/audio_processing_modules/ias_mixer_amp/
				component: mix_media_in
					gain = 0
				component: mix_navigation_in
					gain = 0
					balance = 0
					fader = 1440
				component: mix_hmi_in
					gain = 0
					balance = 0
					fader = 1440

supDomain: VolumeControl
	domainGroup: Amplifier
		domain: MediaStream
			conf: Muted
				VehicleAudioMute Includes Music
				/Audio/smartx/audio_processing_in_place_modules/ias_volume_amp/media/mute/params = 1 100 0

			conf: Unmuted
				/Audio/smartx/audio_processing_in_place_modules/ias_volume_amp/media/mute/params = 0 100 0

		domain: HmiStream
			conf: Muted
				VehicleAudioMute Includes SystemSound
				/Audio/smartx/audio_processing_in_place_modules/ias_volume_amp/hmi/mute/params = 1 100 0

			conf: Unmuted
				/Audio/smartx/audio_processing_in_place_modules/ias_volume_amp/hmi/mute/params = 0 100 0
