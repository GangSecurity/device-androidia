load_audio_modules() {
	insmod $modules/snd-soc-acpi.ko
	insmod $modules/snd-hda-core.ko
	insmod $modules/snd-hda-ext-core.ko
	insmod $modules/snd-soc-sst-match.ko
	insmod $modules/snd-soc-sst-acpi.ko
	insmod $modules/snd-soc-sst-dsp.ko
	insmod $modules/snd-soc-sst-ipc.ko
	insmod $modules/snd-soc-skl-ipc.ko
	insmod $modules/snd-soc-skl-virtio-sst.ko
	insmod $modules/snd-soc-skl.ko
	insmod $modules/snd-soc-tdf8532.ko
	insmod $modules/snd-soc-sst_bxt_tdf8532.ko
	insmod $modules/snd-soc-skl-virtio-fe.ko
	insmod $modules/snd-soc-skl-virtio-card.ko
	setprop vendor.modules.audio.ready true
}

load_audio_modules&
